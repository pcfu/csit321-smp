<div class="control-panel panel d-none">
  <div id="username"><%= current_user.full_name %></div>
  <div id="user-email"><%= current_user.email %></div>

  <%= form_with url: '/logout' do |f| %>
    <div class="text-center">
      <%= f.submit 'Log out', id: 'logout-btn', class: "btn btn-primary" %>
    </div>
  <% end %>
</div>
